dolookthru <- function(building_blocks, bench, lt_year,lt_month, b_year,b_month, Feas_Set)
{
source('/srv/shiny-server/Adex/toolkit/LookThruApp/lookthruclass.R')
#j <- Feas_Set[sample(nrow(Feas_Set), 1), ]
j <- sample(nrow(Feas_Set),1)
print(head(Feas_Set))
print(j)
fundcode <- NULL
fund_name <- NULL
fund_weight <- NULL
fund_risk <- NULL
fund_te <- NULL
for (i in 1:nrow(building_blocks))
{
fundcode[i]    <-  building_blocks[i,"factorid"]
fund_name[i]   <-  building_blocks[i,"factorname"]
fund_weight[i] <-  round(Feas_Set[j,as.character(fundcode[i])],2)
fund_risk[i]   <-  Feas_Set[j,paste(as.character(fundcode[i]),"_R",collapse="",sep="")]
fund_te[i]     <-  Feas_Set[j,paste(as.character(fundcode[i]),"_TE",collapse="",sep="")]
}
w <- data.frame(fundcode,fund_name,fund_weight,fund_risk,fund_te)
print(w)
LookThruCore     <- LookThru$new()
LookThruCore$setConnection(fst[[4]])
fundsize <- 5000000000
LookThruCore$setMembers(w)
LookThruCore$setBenchmark(bench)
LookThruCore$setLookThruDate(lt_year,lt_month)
#  LookThruCore$setLookThruDate(LOOKTHRU_DATE[1],LOOKTHRU_DATE[2])
#  LookThruCore$setBenchmarkDate(LOOKTHRU_DATE[1],LOOKTHRU_DATE[2])
LookThruCore$setBenchmarkDate(b_year,b_month)
LookThruCore$getCombinedHoldings()
print("so even a warning buts its ok")
print(head(LookThruCore$holdings))
LookThruCore$getRiskMetricsDetails()
LookThruCore$getActivePortfolio()
# LookThruCore$getAllStats()  # deprecated
LookThruCore$getCumulativeReturns()
# LookThruCore$getActivePortfolio()
if(LookThruCore$hasportfolioholdings){
daystradevals$daystrade <- 0.4
daystradevals$fund_size <- fundsize
LookThruCore$setDayMultiplier(0.4)
LookThruCore$setFundSize(fundsize)
LookThruCore$getDaysTradeFromHoldings(fundsize)
show("liquidity_table")
}
else
{
LookThruCore$liquidity_table <- "Please select blends with ticker holdings to view liquidity"
hide("liquidity_table")
}
LookThruCore$tracking_error    <- FSBlend$tracking_error
LookThruCore$port_total_return <- FSBlend$total_return
LookThruCore$port_risk         <- FSBlend$risk
return(LookThruCore)
}
