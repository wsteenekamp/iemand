dolookthru <- function(building_blocks, bench, lt_year,lt_month, b_year,b_month,ereturn_df, Feas_Set)
{
#j <- Feas_Set[sample(nrow(Feas_Set), 1), ]
j <- sample(nrow(Feas_Set),1)
print(head(Feas_Set))
print(j)
fundcode <- NULL
fund_name <- NULL
fund_weight <- NULL
fund_risk <- NULL
fund_te <- NULL
for (i in 1:nrow(building_blocks))
{
fundcode[i]    <-  building_blocks[i,"factorid"]
fund_name[i]   <-  building_blocks[i,"factorname"]
fund_weight[i] <-  round(Feas_Set[j,as.character(fundcode[i])],2)
fund_risk[i]   <-  Feas_Set[j,paste(as.character(fundcode[i]),"_R",collapse="",sep="")]
fund_te[i]     <-  Feas_Set[j,paste(as.character(fundcode[i]),"_TE",collapse="",sep="")]
}
w <- data.frame(fundcode,fund_name,fund_weight,fund_risk,fund_te)
print(w)
}
